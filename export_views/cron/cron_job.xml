<?xml version="1.0" ?>
<odoo>
    <data noupdate="1">
        <record id="export_views" model="ir.cron">
            <field name="state">code</field>
            <field name="code" eval="'model.export_views()'"/>
            <field name="name">Export Views:Export views data to an excel</field>
            <field name="numbercall">-1</field>
            <field name="priority">5</field>
            <field name="doall">True</field>
            <field name="interval_type">days</field>
            <field name="interval_number">1</field>
            <field name="model_id" ref="model_export_data_views"/>
            <field name="active">True</field>
        </record>
    </data>
</odoo>