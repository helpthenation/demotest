<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>
        <record id="hr_leave_report_inherit_tree_view" model="ir.ui.view">
            <field name="name">report.hr.holidays.leave_all.inherit.tree</field>
            <field name="model">hr.leave.report</field>
            <field name="inherit_id" ref="hr_holidays.hr_leave_report_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='leave_type']" position="after">
                    <field name="category" invisible="1"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
